{
    "name": "@coinmeca/wallet-provider",
    "version": "0.0.1",
    "keywords": [
        "coinmeca",
        "interface",
        "wallet"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/coinmeca/wallet-sdk"
    },
    "author": "Coinmeca Team <dev@coinmeca.net>",
    "scripts": {
        "check": "npx tsc --watch",
        "build": "yarn clean && npx tsc --build --force",
        "deploy": "npm publish",
        "clean": "rm -rf ./dist ./build ./.next ./node_modules/.cache ./tsconfig.tsbuildinfo"
    },
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.js",
    "files": [
        "./dist"
    ],
    "exports": {
        ".": "./dist/index.js",
        "./*": [
            "./dist/*.js",
            "./dist/*/index.js"
        ]
    },
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index"
            ],
            "*": [
                "./dist/*",
                "./dist/*/index",
                "./dist/index"
            ]
        }
    },
    "dependencies": {
        "axios": "^1.7.7",
        "crypto-js": "^4.2.0",
        "ethereumjs-tx": "^2.1.2",
        "ethereumjs-util": "^7.1.5",
        "ethereumjs-wallet": "^1.0.2",
        "eventemitter3": "^5.0.1",
        "next": "14.2.16",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.2.2",
        "@types/node": "22.7.9",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/telegram-web-app": "^7.8.0",
        "eslint": "9.13.0",
        "tsx": "^4.19.1",
        "typescript": "^5.6.3"
    },
    "packageManager": "yarn@4.5.1"
}
